module DislinBars;


export { printBars };

use DislinSystem: all;
use DislinCanvas: all;

use String: {string };
use Array: {shape, sel};

/******************************************************************************/

void printBars( float[.] vect)
{
  dis = createDisplay();
  printBars( dis, vect);
  destroyDisplay( dis);
}

void printBars( DislinCanvas & dis, float[.] vect)
{
  qplbar( vect, shape( vect)[0]);
}

